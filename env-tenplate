# Legacy SMTP
EMAIL_SERVICE=smtp
SMTP_HOST=smtp.de
SMTP_PORT=465
SMTP_SECURE=true
SMTP_USER=admin@domain.com
SMTP_PASS=myPassword
SMTP_FROM=MyNextEMailApp <admin@domain.com>

# Resend E-Mail Service
#EMAIL_SERVICE=resend
RESEND_API_KEY=myKey
RESEND_FROM=PMyNextEMailApp <noreply@app-resend.dev>

# SendGrid E-Mail Service
#EMAIL_SERVICE=sendgrid
SENDGRID_API_KEY=mySendgridApiKey
SENDGRID_FROM=MyNextEMailApp <noreply@yourdomain.com>